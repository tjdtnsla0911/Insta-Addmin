<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Repository.addminRepository">
<select id="addminUserList" resultType="com.example.demo.model.User">
select * from user
</select>
<select id="addminBoan" resultType="com.example.demo.model.Boan">
